<?php 

	class Alimento{
		private $idAlimento;
		private $nome;		
		private $quantidade;
		private $nomeGrupo;
		private $idGrupo;


		public function getIdAlimento(){
		return $this->idAlimento;
		}
		public function setIdAlimento($idAlimento_){
			$this->idAlimento = $idAlimento_;
		}

		public function getNome(){
		return $this->nome;
		}
		public function setNome($nome_){
			$this->nome = $nome_;
		}

		public function getQuantidade(){
		return $this->quantidade;
		}
		public function setQuantidade($quantidade_){
			$this->quantidade = $quantidade_;
		}

		public function getNomeGrupo(){
		return $this->nomeGrupo;
		}
		public function setNomeGrupo($nomeGrupo_){
			$this->nomeGrupo = $nomeGrupo_;
		}

		public function getIdGrupo(){
		return $this->idGrupo;
		}
		public function setIdGrupo($idGrupo_){
			$this->idGrupo = $idGrupo_;
		}
		

	} 	

	

?>
